@model IEnumerable<SoundSharp_I9AO.Models.ViewModels.Mp3ViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Make)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PriceExBtw)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Btw)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OwnerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mbsize)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Displaywidth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Displayheight)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Make)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PriceExBtw)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Btw)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mbsize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Displaywidth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Displayheight)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
